{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-appdata-read-recursive",
    "fs:allow-appdata-write-recursive",
    "fs:allow-remove",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPDATA/*"
      ]
    },
    "window:allow-minimize",
    "window:allow-close",
    "window:allow-start-dragging",
    "webview:allow-print",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*/"
        },
        {
          "url": "http://*/"
        }
      ]
    },
    "fs:default",
    "dialog:default",
    "http:default"
  ]
}