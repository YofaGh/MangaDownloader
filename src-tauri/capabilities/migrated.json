{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main",
    "splashscreen"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-appdata-read-recursive",
    "fs:allow-appdata-write-recursive",
    "fs:allow-remove",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPDATA/*"
      ]
    },
    "core:window:allow-minimize",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "core:webview:allow-print",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*/"
        },
        {
          "url": "http://*/"
        }
      ]
    },
    "fs:default",
    "dialog:default",
    "http:default"
  ]
}